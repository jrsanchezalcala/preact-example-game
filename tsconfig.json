{
	"compilerOptions": {
		"outDir": "./dist",
		"target": "es5",
		"moduleResolution": "node",
		"module": "commonjs",
		"noImplicitAny": false,
		"removeComments": true,
		"sourceMap": true,
	    "jsx": "react-jsx",
		"jsxImportSource": "preact",
		"esModuleInterop": true

	},
	"suppressImplicitAnyIndexErrors": true,
	"lib": [
		"dom",
		"es2017"
	],
	"baseUrl": ".",
	"exclude": [
		"dist"
	]
}